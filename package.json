{
  "name": "product-management-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link --project-ref qnwapezkyxqzgbvhlidy",
    "supabase:types": "npx supabase gen types --lang=typescript --project-id qnwapezkyxqzgbvhlidy --schema public > database/types.ts",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@formkit/core": "^1.6.9",
    "@formkit/vue": "^1.6.9",
    "@supabase/supabase-js": "2.47.9",
    "@tanstack/vue-table": "^8.20.5",
    "@vueuse/core": "^12.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "iconify-icon": "^2.2.0",
    "lucide-vue-next": "^0.468.0",
    "pinia": "^2.3.0",
    "radix-vue": "^1.9.11",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.13",
    "vue-meta": "3.0.0-alpha.10",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@faker-js/faker": "^9.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "supabase": "^2.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.28.0",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  }
}
